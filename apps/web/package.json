{
  "name": "@agentic-swarm/web",
  "version": "0.0.1",
  "private": true,
  "description": "Agentic Swarm web frontend (React, mobile-first)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@agentic-swarm/config": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^28.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
