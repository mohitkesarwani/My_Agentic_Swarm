    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@agentic-swarm/config": "workspace:*",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^25.2.2",
    "mongodb-memory-server": "^11.0.1",
    "tsx": "^4.19.2",
